<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
 	<PropertyGroup>
		<TargetFramework>net462</TargetFramework>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
		<WarningLevel>4</WarningLevel>
		<CodeAnalysisTreatWarningsAsErrors>true</CodeAnalysisTreatWarningsAsErrors>
		<AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
		<ResolveNugetPackages>true</ResolveNugetPackages>
		<IsTestProject>false</IsTestProject>
	</PropertyGroup>
 	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
		<OutputPath>bin\</OutputPath>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
		<OutputPath>bin\</OutputPath>
	</PropertyGroup>
	<ItemGroup>
	  <None Remove="Resources\Relativity_Sync_Test_Helper.xml" />
	</ItemGroup>
	<ItemGroup>
	  <EmbeddedResource Include="Resources\Relativity_Sync_Test_Helper.xml">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </EmbeddedResource>
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\Relativity.Sync.Tests.Common\Relativity.Sync.Tests.Common.csproj" />
	  <ProjectReference Include="..\Relativity.Sync\Relativity.Sync.csproj" />
	</ItemGroup>
  <!-- Relativity Packages with Release Versioning Schema -->
  <ItemGroup>
    <PackageReference Include="Castle.Windsor" Version="4.0.0" />
    <PackageReference Include="Relativity.Testing.Framework" Version="0.22.0" />
    <PackageReference Include="Relativity.Testing.Framework.Api" Version="0.22.0" />
    <PackageReference Include="Relativity.Testing.Identification" Version="3.5.0" />
    <PackageReference Include="Relativity.Productions.Services.Interfaces" Version="11.2.13" />
  </ItemGroup>
	<ItemGroup>
		<Reference Include="System.Web.Services" />
	</ItemGroup>
</Project>