<link type="text/css" rel="stylesheet" href="~/Content/tooltip.css"/>

<div id="contextualTooltipView" class="modal">
	<div class="modal-content">
		<img class="close" data-bind="click: ok"/>
		<div>
			<h4 data-bind="text: tooltipTitle"></h4>
		</div>
		<ul data-bind="foreach: tooltips">
			<li>
				<span><strong data-bind="text: name"></strong> - <p data-bind="text: description"/> </span>
			</li>
		</ul>
	</div>
</div>
