@using kCura.IntegrationPoints.Web.Extensions
@model kCura.IntegrationPoints.Web.Models.EditPoint


<div class="card smaller-card clear-fix" id="notEnoughPermission">
	<label class="label"><h3>Source Connection</h3></label>
	
	<div class="field-row">

		<div class="">
			
			You do not have sufficient permission to access this page. Please contact your administrator for the correct permissions.
			
		</div>
		<button class="button generic positive float-right" id="cancel">Cancel</button>
	</div>
</div>

@section scripts{
	<script src="~/Scripts/route.js"></script>
	<script src="~/Scripts/core/utils.js"></script>
	<script>
		$("#cancel").on("click", function () {

			var getBasePath = IP.utils.getBasePath();
			var route = IP.redirect.get();
			if (!IP.redirect.isEdit()) {
				route = document.URL.split('CustomPages')[0] + "@Html.Raw(@Model.URL)";
	}
	window.top.location = route;
		});
		$("#bodyContainer").css("background-color", "#c1c1c1");
	</script>
	
}

