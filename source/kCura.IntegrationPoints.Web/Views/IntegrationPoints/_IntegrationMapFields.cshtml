<script id="step3" type="text/html">
	<div class="card" id="fieldMappings">
		<div>
			<span class="label title">Field Mappings</span>
			<span style="visibility: hidden;" id="forceRedraw"></span>
			<div class="map-field-title">
				<label class="list-title workspace-field">Workspace Fields</label>
				<label class="list-title source-field">Source Attributes</label>

			</div>
			<div data-bind="validationMessage: sourceMapped" class="icon-error legal-hold field-validation-error"></div>
			<div class="table" id="mapFields">
				<div class="cell">
					<select id="workspace-fields" class="list-box" name="workspace-field" data-bind="options: workspaceFields, selectedOptions: workspaceFieldSelected, optionsText: 'name', doubleClick: addSelectFields" multiple="multiple"></select>
				</div>
				<div class="cell selection map-move">
					<div>
						<a href="#" id="add-all-workspace-fields" class="icon-move-last" data-bind="click: addAllWorkspaceFields"></a>
						<a href="#" id="add-workspace-field" class="icon-move-next" data-bind="click: addSelectFields"></a>
						<a href="#" id="remove-workspace-field" class="icon-move-previous" data-bind="click: addToWorkspaceField"></a>
						<a href="#" id="remove-all-workspace-fields" class="icon-move-first" data-bind="click: addAlltoWorkspaceField"></a>
					</div>
				</div>
				<div class="cell">
					<select class="list-box" id="selected-workspace-fields" data-bind="options: mappedWorkspace, selectedOptions: selectedMappedWorkspace, optionsText: 'name', doubleClick: addToWorkspaceField" multiple="multiple"></select>

				</div>
				<div class="cell selection map-move">
					<div>
						<a href="#" id="workspaceFieldTop" class="icon-move-to-top" data-bind="click: moveMappedWorkspaceTop"></a>
						<a href="#" id="workspace-field-up" class="icon-move-up" data-bind="click: moveMappedWorkspaceUp"></a>
						<a href="#" id="workspace-field-down" class="icon-move-down" data-bind="click: moveMappedWorkspaceDown"></a>
						<a href="#" id="workspaceFieldBottom" class="icon-move-to-bottom" data-bind="click: moveMappedWorkspaceBottom"></a>
					</div>
				</div>
				<div class="cell selection map-move">
					<div>
						<a href="#" id="attributeFieldTop" class="icon-move-to-top" data-bind="click: moveMappedSourceTop"></a>
						<a href="#" id="attribute-field-up" class="icon-move-up" data-bind="click: moveMappedSourceUp"></a>
						<a href="#" id="attribute-field-down" class="icon-move-down" data-bind="click: moveMappedSourceDown"></a>
						<a href="#" id="attributeFieldBottom" class="icon-move-to-bottom" data-bind="click: moveMappedSourceBottom"></a>
					</div>
				</div>
				<div class="cell">
					<select class="list-box" id="selected-source-fields" multiple="multiple" data-bind="options: sourceMapped, selectedOptions: selectedMappedSource, optionsText: 'name', doubleClick: addToSourceField ">
					</select>
				</div>
				<div class="cell selection map-move">
					<div>
						<a href="#" id="add-all-source-fields" class="icon-move-last" data-bind="click: addAlltoSourceField"></a>
						<a href="#" id="add-source-field" class="icon-move-previous" data-bind="click: addToMappedSource"></a>
						<a href="#" id="remove-source-field" class="icon-move-next" data-bind="click: addToSourceField"></a>
						<a href="#" id="remove-all-source-fields" class="icon-move-first" data-bind="click: addSourceToMapped"></a>
					</div>
				</div>
				<div class="cell">
					<div>
						<select class="list-box" id="source-fields" multiple="multiple" data-bind="options: sourceField, selectedOptions: selectedSourceField, optionsText: 'name', doubleClick: addToMappedSource">
						</select>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="card">
		<span class="label title">Import Settings</span>
		<div class="identifier">
			<div class="field-row overlay-identifier">
				<div class="field-label required">Unique Identifier:</div>
				<div class="field-value ">
					<select id="overlay-identifier" data-bind="options: overlay, value: selectedUniqueId, optionsValue: 'displayName', optionsText: 'displayName', select2: {}">
					</select>
				</div>
			</div>

			<div class="field-row" data-bind="visible: hasParent">
				<div class="field-label required ">Parent Attribute:</div>
				<div class="field-value ">
					<select id="parent-identifier" data-bind="options: parentField, value: selectedIdentifier, optionsValue: 'name', optionsText: 'name', optionsCaption: 'Select...' ,select2: {}">
					</select>
				</div>
			</div>
			<div class="field-row" data-bind="visible: isDocument() == 'true'">
				<div class="field-label required ">Import Native File:</div>
				<div class="field-value ">
					<ul>
						<li>
							<input type="radio" name="import" value="true" data-bind="checked: importNativeFile">Yes</li>
						<li>
							<input type="radio" name="import" value="false" data-bind="checked: importNativeFile">No</li>
					</ul>

				</div>
			</div>
			<div class="field-row" data-bind="visible: importNativeFile() == 'true'">

				<div class="field-label required ">Native File Path:</div>
				<div class="field-value ">
					<select id="native-filePath" data-bind="options: nativeFilePathOption, value: nativeFilePathValue, optionsValue: 'displayName', optionsText: 'displayName', optionsCaption: 'Select...', select2: {}">
					</select>
				</div>
			</div>
			<div class="field-row" data-bind="visible: showManager">
				<div class="field-label required ">Custodian Manager<br/>
					Contains Link:
				</div>
				<div class="field-value ">
					<ul>
						<li>
							<input type="radio" name="enable" value="true" data-bind="checked: CustodianManagerFieldContainsLink">Yes</li>
						<li>
							<input type="radio" name="enable" value="false" data-bind="checked: CustodianManagerFieldContainsLink">No</li>
					</ul>
				</div>
			</div>
			<div class="field-row">
				<div class="field-label required ">Use Folder Path Information:</div>
				<div class="field-value ">
					<ul>
						<li>
							<input type="radio" name="import_createFolder" value="true" data-bind="checked: UseFolderPathInformation">Yes
						</li>
						<li>
							<input type="radio" name="import_createFolder" value="false" data-bind="checked: UseFolderPathInformation">No
						</li>
					</ul>
				</div>
			</div>
			<div class="field-row" data-bind="visible: UseFolderPathInformation() == 'true'">
				<div class="field-label required ">Folder Path Information:</div>
				<div class="field-value ">
					<select id="folderPath" data-bind="options: FolderPathFields, value: FolderPathSourceField, optionsText: 'actualName', optionsValue: 'fieldIdentifier', optionsCaption: 'Select...' ,select2: {}"></select>
				</div>
			</div>
		</div>
		<div data-bind="visible: isDocument() == 'true'">
			<span class="label title">Extracted Text</span>
			<div class="field-row">
				<div class="field-label required ">Field Contains File Location</div>
				<div class="field-value ">
					<ul>
						<li>
							<input type="radio" name="import_extractedTextLocation" value="true" data-bind="checked: ExtractedTextFieldContainsFilePath">Yes
						</li>
						<li>
							<input type="radio" name="import_extractedTextLocation" value="false" data-bind="checked: ExtractedTextFieldContainsFilePath">No
						</li>
					</ul>
				</div>
			</div>
			<div class="field-row" data-bind="visible: ExtractedTextFieldContainsFilePath() == 'true'">
				<div class="field-label required ">Encoding for undetectable files:</div>
				<div class="field-value ">
					<select id="encodings" data-bind="options: ExtractedTextFileEncodingList, value: ExtractedTextFileEncoding, optionsText: 'displayName', optionsValue: 'name', optionsCaption: 'Select...' ,select2: {}"></select>
				</div>
			</div>
		</div>
	</div>
</script>
